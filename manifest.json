{
    "name": "Movies List",
    "description": "Movies tacking app",
    "version": "0.1",
    "manifest_version": 2,
    "app": {
      "background": {
        "scripts": ["background.js"]
      }
    },
    "permissions": [
        "storage",
        "<all_urls>"
    ],
    "icons": { "16": "img/movie-16.png", "128": "img/movie-128.png" }
  }